{
  "name": "project-intern-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/bin/www.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:watch": "tsc --watch",
    "test": "npm run eslint",
    "format": "npm run prettier",
    "prettier": "prettier src/*/*.ts -- --write",
    "eslint": "eslint src/*/*.ts --ignore-pattern src/migrations/*.ts --fix",
    "build": "tsc",
    "start": "npm run build && node -r source-map-support/register .",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "big.js": "^5.2.2",
    "bootstrap": "^4.4.1",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "http-status": "^1.4.2",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "mysql2": "^2.1.0",
    "qs": "^6.9.1",
    "request-ip": "^2.1.3",
    "serve-favicon": "^2.5.0",
    "ts-node": "^8.10.1",
    "typeorm": "^0.2.34",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/big.js": "^4.0.5",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/express-ejs-layouts": "^2.3.1",
    "@types/express-session": "^1.15.11",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/multer": "^1.4.2",
    "@types/node": "^10.17.21",
    "@types/qs": "^6.9.1",
    "@types/request-ip": "0.0.35",
    "@types/serve-favicon": "^2.5.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.2"
  }
}
